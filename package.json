{
  "name": "frontend-coding-challenge-angular",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "postinstall": "yarn generate 2000 313",
    "ng": "ng",
    "start": "concurrently \"yarn server\" \"ng serve\" --names server,client --prefix-colors yellow,blue",
    "server": "nodemon --exec \"ts-node\" server/server.ts --ext ts,json --watch server",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "ng lint",
    "generate": "ts-node generator/generate.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "concurrently": "^3.5.1",
    "core-js": "^2.4.1",
    "express": "^4.16.3",
    "moment": "^2.22.1",
    "rxjs": "^5.5.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular-devkit/core": "0.0.29",
    "@angular/cli": "1.6.3",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/language-service": "^5.0.0",
    "@types/express": "^4.11.1",
    "@types/jest": "23.3.12",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "jest": "^24.8.0",
    "jest-preset-angular": "^7.1.1",
    "nodemon": "^1.17.3",
    "ts-node": "^6.0.2",
    "tslint": "~5.7.0",
    "typescript": "^2.7.2"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/frontend/setupJest.ts"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/frontend/tsconfig.spec.json",
        "stringifyContentPathRegex": "\\.html$",
        "astTransformers": [
          "jest-preset-angular/InlineHtmlStripStylesTransformer"
        ]
      }
    }
  }
}
